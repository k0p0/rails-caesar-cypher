<div class="container text-center">
  <h1>Caesar decrypted possibilities</h1>
</div>



<ol class="list-group">
        <% @decrypted_possibilities.each_with_index do |possibility, index| %>
          <li class="list-group-item">
            <span class="badge"><%= index + 1 %></span>
            <%= possibility %>
          </li>
        <% end %>
      </ol>
